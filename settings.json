{
  "_credit": "Settings customized by BeanGreen247",
  "remote.SSH.remotePlatform": {
    "dev-server": "linux"
  },
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.statusBar.visible": false,
  "breadcrumbs.enabled": false,
  "workbench.startupEditor": "none",
  "git.enableSmartCommit": true,
  "search.maxResults": 2000,
  "simpleBrowser.focusLockIndicator.enabled": false,
  "editor.minimap.enabled": false,
  "editor.codeLens": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.semanticHighlighting.enabled": false,
  "editor.smoothScrolling": false,
  "terminal.integrated.gpuAcceleration": "on",
  "workbench.reduceMotion": "on",
  "files.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/vendor": true,
    "**/dist": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/vendor": true,
    "**/dist": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/vendor/**": true,
    "**/dist/**": true
  },
  "window.autoDetectHighContrast": false,
  "remote.SSH.useLocalServer": true,
  "remote.SSH.showLoginTerminal": true,
  "workbench.editorLargeFileConfirmation": 1024,
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.showEditorActions": false,
  "workbench.editor.enablePreview": false,
  "workbench.tips.enabled": false,
  "workbench.enableExperiments": false,
  "workbench.enableWelcomePage": false,
  "workbench.experimental.useRipgrepSearch": true,
  "workbench.experimental.editor.enablePreviewFromQuickOpen": false,
  "window.showAccounts": "off",
  "window.menuBarVisibility": "compact",
  "window.titleBarStyle": "custom",
  "github.copilot.enable": false,
  "github.copilot.advanced": {
    "inlineSuggestCount": 0
  },
  "eslint.enable": false,
  "stylelint.enable": false,
  "python.linting.enabled": false,
  "pylint.enabled": false,
  "eslint.validate": [],
  "javascript.validate.enable": false,
  "typescript.validate.enable": false,
  "search.quickOpen.includeSymbols": false,
  "extensions.ignoreRecommendations": true,
  "update.mode": "manual",
  "telemetry.telemetryLevel": "off",
  "search.useIgnoreFiles": true,
  "files.maxMemoryForLargeFilesMB": 8192,
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": false,
  "workbench.sideBar.location": "right",
  "workbench.activityBar.location": "top",
  "editor.largeFileOptimizations": true,
  "editor.screenReaderAnnounceInlineSuggestion": false,
  "editor.suggest.filterGraceful": false,
  "editor.suggest.selectionMode": "never",
  "editor.suggest.showClasses": false,
  "editor.suggest.showColors": false,
  "editor.suggest.showConstants": false,
  "editor.suggest.showConstructors": false,
  "editor.suggest.showCustomcolors": false,
  "editor.suggest.showDeprecated": false,
  "editor.suggest.showEnumMembers": false,
  "editor.suggest.showFiles": false,
  "editor.suggest.showFields": false,
  "editor.suggest.showEvents": false,
  "editor.suggest.showEnums": false,
  "editor.suggest.showIcons": false,
  "editor.suggest.showFolders": false,
  "editor.suggest.showFunctions": false,
  "editor.suggest.showInlineDetails": false,
  "editor.suggest.showInterfaces": false,
  "editor.suggest.showIssues": false,
  "editor.suggest.showKeywords": false,
  "C_Cpp.intelliSenseEngine": "disabled",
  "editor.suggest.matchOnWordStartOnly": false,
  "editor.suggest.showMethods": false,
  "editor.suggest.showModules": false,
  "editor.suggest.showOperators": false,
  "editor.suggest.showProperties": false,
  "editor.suggest.showReferences": false,
  "editor.suggest.showStructs": false,
  "editor.suggest.showSnippets": false,
  "editor.suggest.showTypeParameters": false,
  "editor.suggest.showUnits": false,
  "editor.suggest.showVariables": false,
  "editor.suggest.showValues": false,
  "editor.suggest.showUsers": false,
  "editor.suggest.showWords": false,
  "C_Cpp.dimInactiveRegions": false,
  "C_Cpp.errorSquiggles": "disabled",
  "typescript.tsserver.web.projectWideIntellisense.enabled": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "C_Cpp.enhancedColorization": "disabled",
  "window.titleBarStyle": "native",
  "outline.showVariables": false,
  "outline.showFunctions": false,
  "outline.showConstants": false,
  "outline.showEnums": false,
  "outline.showProperties": false,
  "outline.showMethods": false,
  "editor.inlineSuggest.enabled": false,
  "editor.hover.enabled": false,
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  }
}
